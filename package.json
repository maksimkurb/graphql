    "test:integration": "jest --config ./integration/jest-e2e.json"
  },
  "devDependencies": {
    "@nestjs/common": "6.1.1",
    "@nestjs/core": "6.1.1",
    "@nestjs/platform-express": "6.1.1",
    "@nestjs/testing": "6.1.1",
    "@types/graphql": "14.2.0",
    "@types/jest": "24.0.12",
    "@types/node": "12.0.0",
    "apollo-server-express": "2.5.0",
    "class-transformer": "0.2.2",
    "graphql": "14.3.0",
    "husky": "1.3.1",
    "jest": "24.8.0",
    "lint-staged": "8.1.6",
    "prettier": "1.17.0",
    "reflect-metadata": "0.1.13",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "type-graphql": "0.17.0",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "chokidar": "3.0.0",
