    "@nestjs/core": "^6.0.0",
    "graphql": "^14.1.1",
    "reflect-metadata": "^0.1.12"
  },
  "optionalDependencies": {
    "type-graphql": "^0.17.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
